<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">	
        
    <LinearLayout
        android:id="@+id/layout_row_app"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:orientation="horizontal">
        
        <LinearLayout
            android:id="@+id/row_prior_body"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:gravity="bottom"
                android:orientation="horizontal" >

				<LinearLayout 
	                android:layout_width="wrap_content"
	            	android:layout_height="wrap_content"
	            	android:gravity="center_vertical|center_horizontal"
	            	android:orientation="vertical">
		            <LinearLayout 
		                android:id="@+id/mark_sun"
		                android:layout_width="15dp"
	            		android:layout_height="5dp"
	            		android:background="#ff0000"
	            		android:orientation="horizontal"/>
		            <TextView
			            android:id="@+id/txt_app_name_sun"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:textColor="#ff0000"
			            android:text="@string/txt_sun"
			            android:textSize="17sp" 
			            android:textStyle="bold"
			            android:paddingLeft="5dp"
			            android:paddingRight="5dp"
			            android:paddingTop="2dp"/>
	            </LinearLayout>
				<LinearLayout 
	                android:layout_width="wrap_content"
	            	android:layout_height="wrap_content"
	            	android:gravity="center_vertical|center_horizontal"
	            	android:layout_marginLeft="5dp"
	            	android:orientation="vertical">
		            <LinearLayout 
		                android:id="@+id/mark_mon"
		                android:layout_width="15dp"
	            		android:layout_height="5dp"
	            		android:background="#ff0000"
	            		android:orientation="horizontal"/>
		            <TextView
			            android:id="@+id/txt_app_name_mon"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:textColor="#000000"
			            android:text="@string/txt_mon"
			            android:textSize="17sp" 
			            android:textStyle="bold"
			            android:paddingLeft="5dp"
			            android:paddingRight="5dp"
			            android:paddingTop="2dp"/>
	            </LinearLayout>
	            <LinearLayout 
	                android:layout_width="wrap_content"
	            	android:layout_height="wrap_content"
	            	android:gravity="center_vertical|center_horizontal"
	            	android:layout_marginLeft="5dp"
	            	android:orientation="vertical">
		            <LinearLayout 
		                android:id="@+id/mark_tue"
		                android:layout_width="15dp"
	            		android:layout_height="5dp"
	            		android:background="#ff0000"
	            		android:orientation="horizontal"/>
		            <TextView
			            android:id="@+id/txt_app_name_tue"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:textColor="#000000"
			            android:text="@string/txt_tue"
			            android:textSize="17sp" 
			            android:textStyle="bold"
			            android:paddingLeft="5dp"
			            android:paddingRight="5dp"
			            android:paddingTop="2dp"/>
	            </LinearLayout>
	            <LinearLayout 
	                android:layout_width="wrap_content"
	            	android:layout_height="wrap_content"
	            	android:gravity="center_vertical|center_horizontal"
	            	android:layout_marginLeft="5dp"
	            	android:orientation="vertical">
		            <LinearLayout 
		                android:id="@+id/mark_wed"
		                android:layout_width="15dp"
	            		android:layout_height="5dp"
	            		android:background="#ff0000"
	            		android:orientation="horizontal"/>
		            <TextView
			            android:id="@+id/txt_app_name_wed"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:textColor="#000000"
			            android:text="@string/txt_wed"
			            android:textSize="17sp" 
			            android:textStyle="bold"
			            android:paddingLeft="5dp"
			            android:paddingRight="5dp"
			            android:paddingTop="2dp"/>
	            </LinearLayout>
	            <LinearLayout 
	                android:layout_width="wrap_content"
	            	android:layout_height="wrap_content"
	            	android:gravity="center_vertical|center_horizontal"
	            	android:layout_marginLeft="5dp"
	            	android:orientation="vertical">
		            <LinearLayout 
		                android:id="@+id/mark_thu"
		                android:layout_width="15dp"
	            		android:layout_height="5dp"
	            		android:background="#ff0000"
	            		android:orientation="horizontal"/>
		            <TextView
			            android:id="@+id/txt_app_name_thu"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:textColor="#000000"
			            android:text="@string/txt_thu"
			            android:textSize="17sp" 
			            android:textStyle="bold"
			            android:paddingLeft="5dp"
			            android:paddingRight="5dp"
			            android:paddingTop="2dp"/>
	            </LinearLayout>
	            <LinearLayout 
	                android:layout_width="wrap_content"
	            	android:layout_height="wrap_content"
	            	android:gravity="center_vertical|center_horizontal"
	            	android:layout_marginLeft="5dp"
	            	android:orientation="vertical">
		            <LinearLayout 
		                android:id="@+id/mark_fri"
		                android:layout_width="15dp"
	            		android:layout_height="5dp"
	            		android:background="#ff0000"
	            		android:orientation="horizontal"/>
		            <TextView
			            android:id="@+id/txt_app_name_fri"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:textColor="#000000"
			            android:text="@string/txt_fri"
			            android:textSize="17sp" 
			            android:textStyle="bold"
			            android:paddingLeft="5dp"
			            android:paddingRight="5dp"
			            android:paddingTop="2dp"/>
	            </LinearLayout>
	            <LinearLayout 
	                android:layout_width="wrap_content"
	            	android:layout_height="wrap_content"
	            	android:gravity="center_vertical|center_horizontal"
	            	android:layout_marginLeft="5dp"
	            	android:orientation="vertical">
		            <LinearLayout 
		                android:id="@+id/mark_sat"
		                android:layout_width="15dp"
	            		android:layout_height="5dp"
	            		android:background="#ff0000"
	            		android:orientation="horizontal"/>
		            <TextView
			            android:id="@+id/txt_app_name_sat"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:textColor="#0000ff"
			            android:text="@string/txt_sat"
			            android:textSize="17sp" 
			            android:textStyle="bold"
			            android:paddingLeft="5dp"
			            android:paddingRight="5dp"
			            android:paddingTop="2dp"/>
	            </LinearLayout>
	        </LinearLayout>
	        
	        <LinearLayout
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content"
	            android:layout_margin="5dp"
	            android:orientation="horizontal" >
	            
	            <LinearLayout
	                android:id="@+id/layout_status"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_marginLeft="15dp"
	                android:gravity="center_vertical"
	                android:orientation="horizontal" >

		            <TextView
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginRight="5dp"
		                android:singleLine="true"
		                android:text="@string/txt_setting"
		                android:textColor="#0000ff"
		                android:textSize="13sp" />
		            <TextView
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginRight="5dp"
		                android:singleLine="true"
		                android:text=" : "
		                android:textColor="#0000ff"
		                android:textSize="13sp" />
		            <TextView
		                android:id="@+id/txt_prior_lmttime"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginRight="5dp"
		                android:singleLine="true"
		                android:text="01:00"
		                android:textColor="#0000ff"
		                android:textSize="13sp" />
		            <!-- 
		            <TextView
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginRight="5dp"
		                android:singleLine="true"
		                android:text="@string/txt_min"
		                android:textSize="13sp" />
		                -->
		            
		            <TextView
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginRight="5dp"
		                android:singleLine="true"
		                android:text=" / "
		                android:textSize="13sp" />
		            
		            <!-- 잔여 -->
		            <TextView
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginRight="5dp"
		                android:singleLine="true"
		                android:text="@string/txt_spear"
		                android:textColor="#ff0000"
		                android:textSize="13sp" />
		            <TextView
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginRight="5dp"
		                android:singleLine="true"
		                android:text=" : "
		                android:textColor="#ff0000"
		                android:textSize="13sp" />
		            <TextView
		                android:id="@+id/txt_prior_spctime"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginRight="5dp"
		                android:singleLine="true"
		                android:text="01:00"
		                android:textColor="#ff0000"
		                android:textSize="13sp" />
		            <!--
		            <TextView
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginRight="5dp"
		                android:singleLine="true"
		                android:text="@string/txt_min"
		                android:textSize="13sp" />
					-->
	            </LinearLayout>
	            
			</LinearLayout>
		</LinearLayout>	
		
		<LinearLayout
		    android:layout_width="0dp"
		    android:layout_height="fill_parent"
		    android:layout_marginRight="10dp"
		    android:layout_weight="1"
		    android:gravity="center_vertical|right"
		    android:orientation="horizontal" >
			<Button 
			    android:id="@+id/btn_prior_setting"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginRight="10dp"
	            android:background="@drawable/tog_btn_setting_mid_blue" />
	        <Button 
	            android:id = "@+id/btn_prior_delete"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:background="@drawable/tog_btn_delete_small" />
		</LinearLayout>
    </LinearLayout>
    

    
    <!-- Line #e5a25a-->
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:background="#dadada"
        android:focusable="false"
        android:clickable="false"/>
    
</LinearLayout>
